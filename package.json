{
  "name": "@moonbase.sh/embed-nuxt-storefront",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "pnpm run build-styles",
    "build-styles": "tailwindcss -i ./main.css -o ./dist.css",
    "build-styles:watch": "tailwindcss -i ./main.css -o ./dist.css --watch",
    "serve": "serve -p 5005",
    "dev": "run-p build-styles:watch serve"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "serve": "^14.2.3",
    "tailwindcss": "^3.4.10"
  }
}
